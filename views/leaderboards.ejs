<%- include('partials/header') %>

<h1>Leaderboard</h1>
<p>Here are the top 10 streaks:</p>


<table>
    <thead>
        <tr>
            <th>Rank</th>
            <th>Streak</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        <% leaderboards.forEach((entry, index) => { %>
            <tr>
                <td><%= index + 1 %></td> 
                <td><%= entry.streak %></td> 
                <td><%= entry.date %></td> 
            </tr>
        <% }) %>
    </tbody>
</table>


<a href="/quiz">Start New Quiz</a>
<a href="/">Return to Home</a>

</body>
</html>